    "description": "Example showing how to add new file templates to templates manager from vs-file-templates extension using another visual studio code extension",
    "version": "0.0.1",
    "publisher": "visualzoran",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
    },
	"author": {
        "name": "Visual Zoran",
        "url": "https://visualzoran.wordpress.com/"
	},
	"repository": {
		"url": "https://github.com/visualzoran/vz-templates-sample-ext"
	},
    "license": "LICENSE",
    "scripts": {
        "vscode:prepublish": "npm run compile",
